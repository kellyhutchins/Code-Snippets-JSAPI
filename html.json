{
	"JSAPI Resources": {
		"prefix": "jsapi",
		"body": [
			"<link rel=\"stylesheet\" href=\"https://js.arcgis.com/${1:4.3}/esri/css/main.css\">",
			"<script>",
			"var locationPath = location.pathname.replace(/\\/[^\\/]+$/,\"\");",
			"window.dojoConfig = {",
			"packages:[",
			"{",
			"name:\"app\",",
			"location: locationPath + \"/app\"",
			"}",
			"]",
			"}",
			"</script>",
			"<script src=\"https://js.arcgis.com/${2:4.3}\"></script>"
		],
		"description": "Add script and link tags for jsapi"
	},
	"View Div": {
		"prefix": "viewDiv",
		"body": [
			"<div id=\"viewDiv\"></div>",
			"<script> require([\"app/main\"]);</script>"
		],
		"description": "Add viewDiv and require script tag"
	}
}
